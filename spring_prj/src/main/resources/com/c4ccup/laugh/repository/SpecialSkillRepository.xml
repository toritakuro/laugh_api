<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.c4ccup.laugh.repository.SpecialSkillRepository">

  <resultMap id="SpecialSkillResultMap" type="com.c4ccup.laugh.domain.SpecialSkill">
    <id column="id" property="id" />
    <result column="special_skill_name" property="specialSkillName"/>
  </resultMap>

  <select id="getSpecialSkillList" resultMap="SpecialSkillResultMap">
  SELECT 
    id, special_skill_name
  FROM
    special_skill
  </select>
</mapper>