<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.c4ccup.laugh.repository.ComedyStyleRepository">

  <resultMap id="ComedyStyleResultMap" type="com.c4ccup.laugh.domain.ComedyStyle">
    <id column="id" property="id" />
    <result column="style_name" property="styleName"/>
    <result column="create_at" property="createAt"/>
    <result column="update_at" property="updateAt"/>
  </resultMap>

  <select id="getComedyStyleList" resultType="string">
  SELECT DISTINCT
    style_name
  FROM
    comedy_style
  </select>
</mapper>